{
  "name": "New Tab Bookmarks",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Shows bookmarks in the new tab",
  "homepage_url": "https://karlswedberg.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "custom-options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "New Tab Bookmarks"
  },
  "chrome_url_overrides": {
    "newtab": "bookmarks.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ]
}
