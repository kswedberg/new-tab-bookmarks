{
  "name": "new-tab-bookmarks",
  "version": "1.3.0",
  "description": "New Tab Bookmarks",
  "author": "Karl Swedberg <kswedberg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "//build:dev": "NODE_ENV=development webpack --hide-modules",
    "dev": "NODE_ENV=development HMR=true TARGET_BROWSER=chrome webpack --hide-modules --watch",
    "dev-chrome": "NODE_ENV=development TARGET_BROWSER=chrome webpack --watch --mode=development",
    "dev-firefox": "NODE_ENV=development TARGET_BROWSER=firefox webpack --watch --mode=development",
    "dev-opera": "NODE_ENV=development TARGET_BROWSER=opera webpack --watch --mode=development",
    "build-chrome": "NODE_ENV=production TARGET_BROWSER=chrome webpack --hide-modules --mode=production",
    "build-firefox": "NODE_ENV=production TARGET_BROWSER=firefox webpack --hide-modules --mode=production",
    "build-opera": "NODE_ENV=production TARGET_BROWSER=opera webpack --hide-modules --mode=production",
    "build": "yarn run build-chrome && yarn run build-firefox && yarn run build-opera"
  },
  "dependencies": {
    "element-theme-chalk": "^2.13.0",
    "element-theme-default": "^1.4.13",
    "element-ui": "^2.13.0",
    "fmjs": "^4.8.0",
    "vue": "^2.6.11",
    "vuex": "^3.0.1",
    "webextension-polyfill": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.5.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "eslint": "^6.7.2",
    "eslint-config-kswedberg": "^4.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^8.0.0",
    "sign-addon": "^2.0.2",
    "terser-webpack-plugin": "^2.3.0",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-extension-reloader": "^1.1.0",
    "webpack-fix-style-only-entries": "^0.4.0",
    "wext-manifest": "^2.0.1",
    "write-webpack-plugin": "^1.1.0",
    "zip-webpack-plugin": "^3.0.0"
  }
}
