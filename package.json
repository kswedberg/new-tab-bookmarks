{
  "name": "newtab-bookmarks",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./public/",
    "copy": "node ./scripts/copy.js",
    "build:js": "webpack --progress --colors",
    "build:css": "postcss app/css/bookmarks.css -c scripts/postcss.config.js -o public/css/bookmarks.css",
    "build": "npm run clean && npm run copy && npm run build:js && npm run build:css",
    "build:production": "NODE_ENV=production npm run build",
    "watch:js": "webpack --progress --colors --watch",
    "watch:css": "postcss app/css/bookmarks.css -c scripts/postcss.config.js -o public/css/bookmarks.css --watch"
  },
  "keywords": [],
