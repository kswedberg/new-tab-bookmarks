    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babili-webpack-plugin": "^0.1.2",
    "bluebird": "^3.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.1.1",
    "eslint-config-kswedberg": "^1.4.2",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "fs-extra": "^3.0.1",
    "jquery": "^3.2.1",
    "postcss": "^6.0.4",
    "postcss-assets": "^4.1.0",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^2.11.0",
