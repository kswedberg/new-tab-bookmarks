{
  "name": "@kswedberg/new-tab-bookmarks",
  "version": "1.4.0",
  "description": "New Tab Bookmarks, a browser extension",
  "author": "Karl Swedberg <kswedberg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "//build:dev": "NODE_ENV=development webpack --hide-modules",
    "dev": "NODE_ENV=development HMR=true TARGET_BROWSER=chrome webpack --hide-modules --watch",
    "dev-chrome": "NODE_ENV=development TARGET_BROWSER=chrome webpack --watch --mode=development",
    "dev-firefox": "NODE_ENV=development TARGET_BROWSER=firefox webpack --watch --mode=development",
    "dev-opera": "NODE_ENV=development TARGET_BROWSER=opera webpack --watch --mode=development",
    "build-chrome": "NODE_ENV=production TARGET_BROWSER=chrome webpack --hide-modules --mode=production",
    "build-firefox": "NODE_ENV=production TARGET_BROWSER=firefox webpack --hide-modules --mode=production",
    "sign-firefox": "node ./scripts/ff-sign.js",
    "build-opera": "NODE_ENV=production TARGET_BROWSER=opera webpack --hide-modules --mode=production",
    "build": "yarn run build-chrome && yarn run build-firefox && yarn run build-opera"
  },
  "dependencies": {
    "element-theme-chalk": "^2.13.2",
    "element-theme-default": "^1.4.13",
    "element-ui": "^2.13.2",
    "fmjs": "^5.10.1",
    "vue": "^2.6.12",
    "vuex": "^3.5.1",
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime-corejs3": "^7.11.2",
    "archiver": "^4.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "eslint": "^7.4.0",
    "eslint-config-kswedberg": "^4.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.2",
    "sign-addon": "^2.0.6",
    "terser-webpack-plugin": "^3.0.7",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.0",
    "webpack-fix-style-only-entries": "^0.5.1",
    "wext-manifest": "^2.2.0",
    "write-webpack-plugin": "^1.1.0",
    "zip-webpack-plugin": "^3.0.0"
  }
}
